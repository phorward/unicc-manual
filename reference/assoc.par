#!mode      insensitive;

#nonassoc   '=';

#left       '+' '-';
#left       '*' '/';

#right      '!';
#right      '^';

@integer    '0-9'+ ;

start$      -> expr* '\n'
            ;

expr        -> expr '=' expr
            |  expr '+' expr
            |  expr '-' expr
            |  expr '*' expr
            |  expr '/' expr
            |  expr '^' expr
            |  '!' expr
            |  '(' expr ')'
            |  @integer
            ;
